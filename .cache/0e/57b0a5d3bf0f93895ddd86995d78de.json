ated":{"line":80,"column":7},"source":"App.js","original":{"line":82,"column":8}},{"generated":{"line":81,"column":0},"source":"App.js","original":{"line":84,"column":0}},{"generated":{"line":82,"column":0},"source":"App.js","original":{"line":84,"column":1}},{"generated":{"line":82,"column":15},"source":"App.js","original":{"line":86,"column":15},"name":"App"},{"generated":{"line":82,"column":18},"source":"App.js","original":{"line":86,"column":18}},{"generated":{"line":83,"column":0},"source":"App.js","original":{"line":86,"column":18}}],"sources":{"App.js":"import hotBg from './assets/hot.jpg';\nimport coldBg from './assets/cold.jpg';\nimport Descriptions from './components/Descriptions';\nimport { useEffect, useState } from 'react';\nimport { getFormattedWeatherData } from './weatherService';\nimport k from './main.js';\n\nfunction App() {\n\n  const canvasRef = useRef(null);\n\n  const [city, setCity] = useState('Paris');\n  const [weather, setWeather] = useState(null);\n  const [units, setUnits] = useState('metric');\n  const [bg, setBg] = useState('hotBg')\n\n\n  useEffect(() => {\n\n    k(canvasRef);\n\n    const fetchWeatherData = async () => {\n      \n      const data = await getFormattedWeatherData(city, units);\n      setWeather(data);\n\n      const threshold = units === \"metric\" ? 20 : 60;\n      if (data.temp <= threshold) {\n        setBg(coldBg);\n      }\n      else setBg(hotBg)\n    }\n\n    fetchWeatherData();\n\n  }, [city, units]) \n\n  const handleUnitsClick = (e) => {\n    const button = e.currentTarget;\n    const currentUnit = button.innerText.slice(1);\n\n    const isCelsius = currentUnit === \"C\";\n    button.innerText = isCelsius ? \"°F\" : \"°C\";\n    setUnits(isCelsius ? \"metric\" : \"imperial\");\n  };\n\n  const enterKeyPressed = (e) => {\n    if (e.keyCode === 13) {\n      setCity(e.currentTarget.value)\n      e.currentTarget.blur();\n    }\n  }\n\n  return (\n    <div className=\"app\" style={{ backgroundImage: `url(${bg})` }}>\n    <div className='overlay'>\n    <canvas style={{ display: \"inline-block\" }} ref={canvasRef}></canvas>\n    { weather && (\n            <div className='container'>\n        \n            <div className='section section__inputs'>\n              <input onKeyDown={enterKeyPressed} type=\"text\" name=\"city\" placeholder='Enter city name...' />\n              <button onClick={(e) => handleUnitsClick(e)}>°F</button>\n            </div>\n    \n            <div className='section section__temperature'>\n              <div className='icon'>\n                <h3>{`${weather.name}, ${weather.country}`}</h3>\n                <img src={weather.iconURL} alt='weatherIcon' />\n                <h3>{weather.description}</h3>\n              </div>\n              <div className='temperature'>\n                <h1>{`${weather.temp.toFixed()} °${units === 'metric' ? 'C' : 'F'}`}</h1>\n              </div>\n            </div>\n    \n            <Descriptions weather={weather} units={units} />\n    \n          </div>\n    )}\n    </div>\n  </div>\n  );\n}\n\nexport default App;\n"},"lineCount":null}},"error":null,"hash":"cbb538703c457076e09eb5ea0786bbca","cacheData":{"env":{}}}